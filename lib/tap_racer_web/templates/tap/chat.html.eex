<div class="container-fluid">
  <div class="container-fluid">
    <h3>
      TapRacer [<%= @room %>] <small><a href="/">back</a></small>
    </h3>
  </div>
  <div class="well" id="messages" data-room="<%= @room %>">
    <%# $messages.each do |message| %>
      <%#= "<p><b>#{message['name']}:</b> #{message['text']}</p>" %>
    <%# end %>
  </div>
  <div class="container-fluid" id="controls">
    <form>
      <input class="btn btn-default" id="send" tabindex="2" type="submit" value="Send"></input>
      <div id="text-group">
        <input autocomplete="off" class="form-control" id="text" name="text" tabindex="1" type="text"></input>
      </div>
    </form>
  </div>
</div>
